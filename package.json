{
  "name": "react-widgets",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
    "dev-server": "babel-node -- ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack-dev-server.config.js --progress --colors --port 2992 --inline",
    "hot-dev-server": "babel-node -- ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack-hot-dev-server.config.js --hot --progress --colors --port 2992 --inline",
    "build": "babel-node -- ./node_modules/webpack/bin/webpack.js --config webpack-production.config.js --progress --profile --colors",
    "watch-prerender": "babel-node -- ./node_modules/webpack/bin/webpack.js --watch --config webpack-dev-prerender.config --progress --profile --colors",
    "server": "pm2-dev ./build/server/server.js",
    "test": "babel-node -- ./node_modules/karma-cli/bin/karma start",
    "lint": "eslint ./src/"
  },
  "description": "Project template for building isomorphic widgets using reactjs",
  "main": "index.js",
  "author": "Bogdan Savluk <savluk.bogdan@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.6.14",
    "css-loader": "^0.19.0",
    "eslint": "^1.0.0",
    "eslint-plugin-react": "^3.2.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "html-loader": "^0.3.0",
    "jasmine-core": "^2.3.0",
    "karma": "^0.13.6",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "0.1.1",
    "karma-firefox-launcher": "^0.1.3",
    "karma-jasmine": "^0.3.6",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.5.1",
    "null-loader": "^0.1.1",
    "react-hot-loader": "^1.2.8",
    "react-proxy-loader": "^0.3.4",
    "source-map-support": "^0.3.2",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.10.1"
  },
  "dependencies": {
    "babel-core": "^5.6.15",
    "babel-loader": "^5.3.1",
    "babel-runtime": "^5.6.15",
    "body-parser": "^1.13.2",
    "classnames": "^2.1.3",
    "debug": "^2.1.3",
    "di1": "^0.1.1",
    "express": "^4.13.1",
    "lodash": "^3.10.0",
    "morgan": "^1.6.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "rx": "^4.0.0",
    "rx-combine-latest-obj": "^1.0.2",
    "rx-react-container": "^0.1.0",
    "superagent": "^1.2.0"
  }
