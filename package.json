{
  "name": "react-widgets",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "dev-server": "webpack-dev-server --config webpack-dev-server.config.js --progress --colors --port 2992 --inline",
    "hot-dev-server": "webpack-dev-server --config webpack-hot-dev-server.config.js --hot --progress --colors --port 2992 --inline",
    "build": "webpack --config webpack-production.config.js --progress --profile --colors --bail",
    "lint": "eslint ./src/",
    "watch-dev": "node dev-server.js",
    "watch-prod": "node dev-server.js --with-prerender",
    "start": "node ./build/server/server.js",
    "test": "karma start"
  },
  "description": "Project template for building isomorphic widgets using reactjs",
  "main": "index.js",
  "author": "Bogdan Savluk <savluk.bogdan@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.0.1",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.6.0",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.0",
    "jasmine-core": "^2.3.0",
    "karma": "^1.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.5.1",
    "nodemon": "^1.9.2",
    "null-loader": "^0.1.0",
    "postcss-loader": "^0.9.1",
    "react-hot-loader": "^1.2.7",
    "react-proxy-loader": "^0.3.4",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
